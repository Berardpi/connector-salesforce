- @organizations_data.each do |organization_data|
  %h3
    Maestrano Organization #{organization_data[:uid]} - #{organization_data[:name]}
    - if organization_data[:last_synchronization]
      \- Last synch: #{organization_data[:last_synchronization].status} #{organization_data[:last_synchronization].updated_at}
      - if organization_data[:last_synchronization].status == "ERROR"
        Error_msg: #{organization_data[:last_synchronization].message}
  %h4 SalesForce Accounts
  - if organization_data[:salesforce_organizations]
    - organization_data[:salesforce_organizations].each do |salesforce_organization|
      = salesforce_organization.Id
      \- #{salesforce_organization.Name} - #{salesforce_organization.Industry}
      %br
  %h4 Connec Organization
  - if organization_data[:connec_organizations]
    - organization_data[:connec_organizations].each do |connec_organization|
      = connec_organization['id']
      \- #{connec_organization['name']} - #{connec_organization['industry']}
      %br
